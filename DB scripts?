CREATE TABLE USERS {
Email NVARCHAR (50) Primary Key,
Password VARCHAR (255),
Salt VARCHAR (255),
UserStatus NVARCHAR (50),
Major NVARCHAR (50),
Class NVARCHAR (50),
Gender NVARCHAR (50)
}

CREATE TABLE QUESTIONS{
SurveyID INT,
SurveyName NVARCHAR(50),
QuestionID INT,
QuestionNumber INT,
QuestionType NVARCHAR(50),
Question (250),
PRIMARY KEY (FormName, QuestionID, SurveyID)
}

CREATE TABLE RESPONSES{
Email NVARCHAR(50),
FormName NVARCHAR(50),
QuestionID INT,
Answer NVARCHAR (250),
PRIMARY KEY (Email, FormName, QuestionID)
}

CREATE TABLE SURVEYS{
SurveyID INT AUTOINCREMENT PRIMARY KEY,
SurveyName NVARCHAR(50)
}
